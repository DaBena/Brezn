{
  "name": "brezn-mobile",
  "version": "0.1.0",
  "description": "Brezn - Dezentrale anonyme Feed-App",
  "main": "App.tsx",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && mkdir -p build && xcodebuild -workspace Brezn.xcworkspace -scheme Brezn -configuration Release -destination 'generic/platform:iOS' -archivePath build/Brezn.xcarchive archive",
    "test:android": "react-native run-android --variant=debug",
    "test:ios": "react-native run-ios --simulator='iPhone 14'"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "^53.0.20",
    "react": "18.2.0",
    "react-dom": "^18.3.1",
    "react-native": "0.72.6",
    "react-native-background-task": "^0.2.1",
    "react-native-camera": "^4.2.1",
    "react-native-device-info": "^10.11.0",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-permissions": "^3.10.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-tor": "^0.1.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/react-native": "^12.5.1",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "keywords": [
    "brezn",
    "p2p",
    "tor",
    "anonymous",
    "feed",
    "decentralized"
  ],
  "author": "Brezn Team",
  "license": "GPL-3.0"
}
